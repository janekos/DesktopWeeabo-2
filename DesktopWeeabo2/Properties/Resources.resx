  <data name="CreateAnimeTable" xml:space="preserve">
    <value>CREATE TABLE 'anime_entries' (
                    'id' integer not null primary key,
                    'mean_score' integer default 0,
                    'episodes' integer default 0,
                    'duration' integer default 0,
                    'type' string default '',
                    'format' string default '',
                    'status' string default '',
                    'description' string default '',
                    'genres' string default '',
                    'synonyms' string default '',
                    'title_english' string default '',
                    'title_romaji' string default '',
                    'title_native' string default '',
                    'start_date' text default '0000-00-00',
                    'end_date' text default '0000-00-00',
                    'cover_image' string default '',
                    'viewing_status' string default '',
                    'personal_score' integer default 0,
                    'personal_review' string default '',
                    'watch_priority' integer default 0,
                    'rewatch_count' integer default 0,
                    'current_episode' integer default 0,
                    'date_added' text default '0000-00-00')</value>
  </data>
  <data name="CreateMangaTable" xml:space="preserve">
    <value>CREATE TABLE 'manga_entries' (
                    'id' integer not null primary key,
                    'mean_score' integer default 0,
                    'volumes' integer default 0,
                    'chapters' integer default 0,
                    'type' string default '',
                    'format' string default '',
                    'status' string default '',
                    'description' string default '',
                    'genres' string default '',
                    'synonyms' string default '',
                    'title_english' string default '',
                    'title_romaji' string default '',
                    'title_native' string default '',
                    'cover_image' string default '',
                    'reading_status' string default '',
                    'personal_score' integer default 0,
                    'personal_review' string default '',
                    'read_priority' integer default 0,
                    'reread_count' integer default 0,
                    'current_chapter' integer default 0,
                    'date_added' text default '0000-00-00')</value>
  </data>
  <data name="SearchAnimeQuery" xml:space="preserve">
    <value>query ($id: Int, $page: Int, $search: String, $sort: [MediaSort]) {
	Page(page: $page, perPage: 50) {
		pageInfo { 
			total
			hasNextPage
		}
		media(id: $id, type: ANIME, search: $search, sort: $sort) {
			id 
			title { 
				english 
				romaji
				native
			}
			synonyms 
			genres
			type 
			meanScore
			format 
			status 
			description(asHtml: false) 
			startDate{
				year
				month
				day
			}
			endDate{
				year
				month
				day
			}
			episodes 
			duration 
			coverImage{ large }
		}
	}
}</value>
  </data>
  <data name="SearchMangaQuery" xml:space="preserve">
    <value>query ($id: Int, $page: Int, $search: String, $sort: [MediaSort] ) {
	Page(page: $page, perPage: 50) {
		pageInfo { 
			total
			hasNextPage
		}
		media(id: $id, type: MANGA, search: $search, sort: $sort) {
			id 
			title { 
				english 
				romaji
				native
			}
			synonyms 
			genres 
			meanScore
			type
			format 
			status 
			description(asHtml: false) 
			volumes
			chapters
			coverImage{ large }
		}
	}
}</value>
